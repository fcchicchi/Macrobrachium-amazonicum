units.tW_CC = {'d', 'g'}; label.tW_CC = {'time', 'weight'};  
temp.tW_CC = C2K(28);  units.temp.tW_CC = 'K'; label.temp.tW_CC = 'temperature';
bibkey.tW_CC = 'MoraesRiodades2005';
