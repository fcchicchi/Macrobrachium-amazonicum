units.WwN_F = {'g', '#'}; label.WwN_F = {'total wet weight', 'total number of eggs/offspring'};  
temp.WwN_F = C2K(30);  units.temp.WwN_F = 'K'; label.temp.WwN_F = 'temperature';
bibkey.WwN_F = 'Hayd2013';
